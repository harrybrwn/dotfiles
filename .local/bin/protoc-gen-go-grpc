#!/usr/bin/sh

set -eu

if [ -x "$GOPATH/bin/protoc-gen-go-grpc" ]; then
  "$GOPATH/bin/protoc-gen-go-grpc" "$@"
elif [ -x /usr/local/bin/protoc-gen-go-grpc ]; then
  /usr/local/bin/protoc-gen-go-grpc "$@"
elif [ -x /usr/bin/protoc-gen-go-grpc ]; then
  /usr/bin/protoc-gen-go-grpc "$@"
elif [ -x "$HOME/.local/bin/protoc-gen-go-grpc" ]; then
  "$HOME/.local/bin/protoc-gen-go-grpc" "$@"
else
  go tool protoc-gen-go-grpc "$@"
fi
